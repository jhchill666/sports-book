<% if (typeof competitions !== "undefined") { %>
    <div class="market-cols include">

        <section class="competitions">
            <header>
                <h2>Competitions</h2>
            </header>

			<ul>
				<% _.each(competitions, function(countryLevel){%>
					<li>
						<header>
                        	<h3><a href="#<%=sport%>/country/<%=countryLevel.id%>"><%=countryLevel.name%></a></h3>
						</header>
						
						<ul>
							<% _.each(countryLevel.competition, function(next){ %>
								
								<% var compLength = countryLevel.competition.length; %>
								
								<li class="<%if(compLength == 1){%>oneComp<%}if(compLength == 2){%>twoComp<%}%>">
							        <a href="#<%=sport%>/competition/<%=next.id%>">
							        	<%=next.name %>
									</a>
								</li> 
								
							<% }) %>
						</ul>
						
                    </li> 
				<% }) %>
			</ul>

        </section>

