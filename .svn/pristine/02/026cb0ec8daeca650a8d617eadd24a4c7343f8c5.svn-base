@media all and (max-width: 1024px) {

	#header-main{
	
		h1{
			position: absolute;
			top: 15px;
			margin-left: 2px;
		}
		
		
		
		.user{
			width: 30%;
			padding: 1px;
			position: absolute;
			margin: 4px 0 0 0;
			margin-top: 14px;
			
			&.logged-in{
			.user-account{
				font-size: 12px;
				margin-top: 0px;
				position: absolute;
				top: 28px;
				width: 260px;
				right: 0px;
				display: none;
				z-index: 9999;
				background: @blue1;
				padding: 5px 0;
				transition-delay: 1s;
			}
				.details{
					font-size: @size-s;	
					margin-left:0;
					padding: 6px 5px;
				}
			}
		}
		.nav-global{
			ul{
				width:65%;
				max-height: 39px;
				margin-left:185px;
				overflow:hidden;
				li{
					a{
						padding: 7px 15px 16px 16px;
					}
				}
				
			}
			
			.sign-in-cta{
	
				.btn-grey, .btn{
					width:52px;
					font-size: @size-s;
					padding:2px 0px;
					height:20px;
					padding:0;
				}
			}
		}
		
		.nav-main {
			
			margin: 0 0 0 185px;
			height:36px;
			.nav-main-container{
				height:36px;
			}
			ul{
				padding-left:0;
				z-index: 99;
				display:block;
				right: auto;
				.dropList(32px, @grey4, 120px);

				&.date-drop-min{
					
					.pill(@grey4, @grey11, 26px, 120px);
					left: 204px;
					padding-left: 12px;
					text-align:left;
					position:absolute;
					top: 5px;
					left: 5px;
					>li{
						padding:8px 0;
						width:100%;
						ul {
							top:27px;
							border-top: 4px solid #2f2f2f;
							left: 0;
							
							li{
								a{
									padding:10px 12px;
								}
							}
						}
						>a{
							border-left:none;
							padding:0;
						}
					}

				}
				
				
				&.price-format-min{
					top:5px;
					
					>li{
						width:100%;
					}
					ul {
						display:none;
						top:27px;
						border-top: 4px solid #2f2f2f;
					}
					
				}
				
				
				&.nav-sport-min{
				display:none;
					.pill(@blue1, @white, 30px, 100px);
					left: 412px;
					a{
						color:@white;
					}
					>li{
						padding:9px 0;
						width:100%;
						ul{
						text-align: left;
						}
						a{
							border-left:none;
						}
					}
					&:hover{
							background: @grey4;
							.pill-nav-hover(@height: 45px, @top: 54px);			
					}	
				}
				&.all-sports-min{
					.pill(@blue1, @white, 30px, 100px);
					left: 412px;
					a{
						color:@white;
					}
					>li{
						padding:9px 0;
						width:100%;
						ul{
						text-align: left;
						}
						a{
							border-left:none;
						}
					}
					&:hover{
							background: @grey4;
							.pill-nav-hover(@height: 45px, @top: 54px);			
					}	
					display:none;
				}
				

				&.date-drop, &.price-format, &.nav-sport, &.nav-sport-min{
					display:none;
				}
				
				
			}   /* ul end */
			
			
			

		} /* nav-main end	 */
		
		.betslipToggle {
			position:absolute;
			height: auto;
			min-width: 30px;
			top:55px;
			right:18px;
			font-weight: bold;
			font-size: 12px;
			color: @white;
			display: block;
			background: @blue1;
			padding: 8px 13px;
			.border-radius(3px);
			.addTransitions(0.5s);
			&.show{
				background: @blue9;
	
			}
		}
	}
}


@media all and (max-width: 960px) {
	

	#header-main{ 
		.nav-global{
			ul{
			width:53%;

			}
			.sign-in-cta{
				margin-top:10px;
				
				.btn, .btn-grey{
					font-size: 12px;
					width: 80px;
					height: 25px;
				}
			}
		}

	
	}

	.betslipToggle {
		height: 19px;
		min-width: 30px;
		font-weight: 500;
		font-size: @size-s;
		color: @grey7;
	}
	


}


@media all and (max-width: 648px) {


		#header-main{
			h1{
				position: static;
				margin: 0 auto;
				margin-top: 4px;
			}
			i{
				margin-left: 5px;
				margin-top: -2px;
			}
		
		.show-menu {
			background-color:@color-nav-global-link-active;
			position: absolute;
			width: 46px;
			padding: 9px 2px;
			height: 40px;
			top: 6px;
			display:block;
		}

		.user{
			padding:3px 0;
			clear:both;
			top:10px;
			width:50%;
			z-index:10000;
			opacity:0;
			.anyTransition(all, 0.6s, linear);
			&.show{
