@import  "variables";
@import  "mixins";


#header-main{
	color: white;
	background-size: 18%;
	i{
		margin-left: 2px;		
	}
		
	.show-menu {
		font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
		text-decoration: none;
		color: #fff;
		background-color: @color-nav-global-link;
		text-align: center;
		padding: 10px 0;
		font-size: 14px;
		width:45px;
		cursor:pointer;
		display:none;
		i{
			font-size:19px;
			margin: 0 auto;
			margin-top:2px;
		}

	}	
	#all-menu{ 
			position: absolute;
			/* box-shadow: 0px 0px 14px #000000; */
			width:auto;
			height:0px;
			overflow: hidden;
			opacity: 1;
			filter: alpha(opacity=00);
			z-index: 9999;
			.opacity-98();
			top: 49px;	
			right: 10px;
			left: 10px;
			bottom:0;
			.addTransitions(0.5s);
				
			> div {
				min-width:300px;
				min-height:100px;
				width:auto;
				ul{
						li{
							&:first-child{
								border: none;
								padding: 18px 10px;
								background: @grey3;
							}
							&:last-child{
								border:none;
							}
							&:not(first-child){
							
							padding:8px 12px 10px 12px;
							border-bottom:1px solid @grey3;
							background: -moz-linear-gradient(top,  rgba(0,0,0,0) 0%, rgba(24,24,24,0.3) 93%); /* FF3.6+ */
							background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0)), color-stop(93%,rgba(24,24,24,0.3))); /* Chrome,Safari4+ */
							background: -webkit-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(24,24,24,0.3) 93%); /* Chrome10+,Safari5.1+ */
							background: -o-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(24,24,24,0.3) 93%); /* Opera 11.10+ */
							background: -ms-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(24,24,24,0.3) 93%); /* IE10+ */
							background: linear-gradient(to bottom,  rgba(0,0,0,0) 0%,rgba(24,24,24,0.3) 93%); /* W3C */
							filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00000000', endColorstr='#4d181818',GradientType=0 ); /* IE6-9 */
							}
							
							color:@blue1;
							font-size:18px;
							line-height:13px;
							font-weight:bold;
							a{
								color:@white;
								font-size: 13px;
								font-weight: bold;
							}
						}
					}

			}
			
			&.show {
				width:auto;
				height:100%;
				opacity:1;
				filter: alpha(opacity=100); /* For IE8 and earlier */
				>div{
				
				}
			}
	} 

	.header-content{
		position: relative;
	}
	

	h1{
		position: fixed;
		top: 19px;
		margin-left: 15px;
		height: 23px;
		width: 164px; 
		background-size: 164px 23px;
		background-repeat: no-repeat;
		display: block;
		background-image: url("@{image-path}/logo.gif");
		text-indent: -9999px;
		white-space: nowrap;
		
		&.absolute{
			position:absolute;
		}
		
	}
	
	.header-image{
		display: none;
		width: 25%;
		min-height: 194px;
		position: absolute;
		top: -10px;
		left: 32.5%;
		text-indent: -99999px;
		background: url("@{image-path}/bg-racing.png") 50% 0 no-repeat;
	}

	.banner{
		padding-bottom: 40px;
	}

	.promo{
		min-height: 114px;
		padding: 0 0 0 60%;
		background: fade(@color-highlight, 30%);
		margin-bottom: 20px;
		display: none;

		img{
			display: none;
		}
	}

	&.show-promo{

		min-height: 188px;
		
		h1{
			top: 200px;
		}

		.header-image{
			display: block;
		}

		.promo{
			display: block;
			
			.remove{
				.pill();
				position: absolute;
				right: 15px;
				margin-top: 15px;
				padding-top: 0.438%;
				color: @color-link;
				&:after{
					font-family: fontAwesome;
					content: "\f00d";
				}
				span{
					display: inline-block;
					text-indent: -99999px;
				}
			}


			img{
				display: block;
			}
		}
	}

	.sign-in-cta{
		position:absolute;
		top: 4px;
		-webkit-transition: all 1s ease-in-out;
			-moz-transition: all 1s ease-in-out;
			-o-transition: all 1s ease-in-out;
			transition: all 1s ease-in-out;	
			
		.btn-grey{
			.btn(@color-nav-main-bg, @color-link, @white);
			width: 80px;
			display: inline-block;
			margin: 0 5px;
			border: none;
			float: right;
		}
	
		.btn{
			.btn(@color-link, @color-link, @white);
			width: 80px;
			display: inline-block;
			margin: 0 5px;
			border: none;
			float: right;
		}

		.join{
			float: right;
		}

		a{
			color: @white;
			font-weight: bold;
		}
	}


	.nav-global{
		.clearfix();
		background: transparent;
		
		
 	
		
		ul{
			.create-percentage-page-wrapper(700px);
			text-align: left;
			display: block;
			float: left;
			margin-left: @width-side-panel-1-px + @wrapper-page-padding-px;
			max-height: 52px;
		//	overflow:hidden;

			> li{
				display: inline-block;
				width: auto;
				padding: 0;
				font-size: @size-l;
				line-height: 1.3em;
				font-weight: bold;

				a{
					color: @color-nav-global-link;
					padding: 12px 24px 16px 24px;
					display: inline-block;
					position:relative;

					&:hover{
						text-decoration: none;
						.opacity-70();
					}
					
					&.active{
						
					//	border-top: 4px solid @blue1;
						background-color:@grey3;
						
						&:before{
							content: '';
							position: absolute;
							background-color: @blue1;
							top: 0px;
							left: 0;
							right: 0;
							height: 4px;
							width: 100%;
						}
						
					}

				}
			}
		}
	}

	.user{
		.create-percentage-page-wrapper(280px);
		padding: 10px 0 0 14px;
		position: absolute;
		right:10px;
		
		.details{
			.btn(@grey5, @grey5, @white);
			width: auto;
			line-height: 1.6em;
			text-align: left;
			margin-left: 15%;
			display: none;
			
			span {
				float: right;
				margin-right: @wrapper-page-padding-px;
			}

			&:hover{
				background: @color-highlight;
				border-color: @color-highlight;
				color: @white;
			}
			
			&:before {
				content: '\f078';
				font-family: fontAwesome;
				float: right;
			}
		}
		&.logged-in{
			width:320px;
			padding: 4px 0 0 0;
			.details{
				display: block;
				margin-left:0;
			}
			
			&:hover{
				.details{
					padding: 2px 10px 4px 10px;
					background: @color-highlight;
					border-color: @color-highlight;
					color: @white;
				}

				.user-account{
					display: block;
					border-top: 1px solid #307BA0;
					top: 32px;
				}
			}

			.user-account{
				font-size: @size-m; 
				margin-top: 2px;
				position: absolute;
				width: 320px;
				top: 35px;
				right: 0px;
				// overflow: hidden;
				display: none;
				z-index: 9999; 
				background: @color-highlight;
				padding: 5px 0;
				transition-delay: 1s;
				li{
					margin: 0;
					padding: 0;
					display: block;
					width: 100%;
					text-align: left;

					&:first-child{
						border-top: none;
					}

					a{	
						font-weight: bold;
						font-size:12px;
						color: @white;
						display: block;
						background: @color-highlight;
						padding: 5px 12px;

						&:hover{
							border: none;
							text-decoration: underline;
						}
						
						
					}
				}
			}
		}



		
	}

	.nav-all{
		display: none;
		padding: 20px 15%;
		background: @color-nav-all-bg;

		ul{
			width: 100%;
			margin: 0 auto;
			-webkit-column-count: 5;
	 		-webkit-column-gap: 10px;
	 		-moz-column-count: 5;
	 		-moz-column-gap: 10px;	

	 		li{
 				padding-bottom: 6px;
	 		}


	 		a{
	 			color: @color-nav-all-link;
	 			font-weight: bold;
	 		}
		}	
	}

	.nav-main {
		z-index: 10;
		position: relative;
		height: 41px;
		//background: @color-nav-main-bg;
		.clearfix;
		margin-left: @width-side-panel-1-px + @wrapper-page-padding-px;
		margin-right: @width-side-panel-2-px + @wrapper-page-padding-px;
		.panel-control-1{
			&:after{
				content:".";
				visibility: hidden;
			}
		}

		.panel-control-1,
		.panel-control-2{
			float: left;
			//width: 22.3%;
			font-size: @size-xl;
			.transition-ease-in();

			a{
				display: inline-block;
				background: @color-nav-main-control;
				color: @color-nav-main-control-text;
				height: 45px;
				padding: 10px 14px 0 14px;
				border-right: 2px solid @black;
			}
		}

		.panel-control-2{
			float: right;
			width: 0%;
			background: red;
			a{
				float: right;
				border-left: 2px solid @black;
			}
		}

		.active-right &{
			.panel-control-1,
			.panel-control-2{
				//background: blue;
			}

			.panel-control-1{
				width: 0%;
			}

			.panel-control-2{
				width: 25.5%;
			}

			.nav-main-container{
				width: 74.5%;
			}

			.toggle{
				margin-left: 0;
			}
		}

		.nav-main-container{
			width: auto;
			position:relative;
			background: @color-nav-main-bg;
			//float: left;
			.clearfix();
			min-height: 41px;
			border-bottom: 1px solid #222;

			.toggle{
				height: 45px;
				width: 45px;
				background: @grey5;
				display: block;
				color: @white;
				font-size: @size-l;
				padding: 11px 0 0 12px;
				float: left;
				border-right: 2px solid @black;
				margin-left: -1px;


				&.bet-slip{
					float: right;
					border: none;
					border-left: 2px solid @black;
					position: relative;
					
					.active-right &{
						margin-right: -2px;
					}

					em{
						position: absolute;
						font-size: @size-xxs;
						min-width: 20px;
						height: 20px;
						background: @color-highlight;
						border-radius: 9px;
						display: table-cell;
						vertical-align: middle;
						text-align: center;
						line-height: 20px;
						padding: 0 6px;
						top: 2px;
						right: 2px;
						font-weight: bold;
					}

				}
			}
		}

		ul{
			
			padding-left: 8px;
			float: left;
			height: 41px;
			.addTransitions(0s);
			// .dropList(32px);
			
			&.all-sports-min, &.nav-sport-min, &.date-drop-min{
					display:none;
			}
				&.price-format-min{
					.pill(@grey4, @grey11, 26px, 120px);
					.dropList(28px, @grey4, 120px);
					margin-left: 15px;
					position: absolute;
					right: 8px;
					float: none;
					top: 7px;
					padding-left: 12px;
					text-align:left;
					z-index: 100;
					
					
					
					ul:hover {
						cursor:pointer;
					}
					ul{
						left:0;
						padding-top: 6px;
						border-top:0;
						top: 25px;
					}
					 
					>li{
						padding:8px 0;
						width:100%;
						ul{
						text-align: left;
						li{
								a{
									padding:10px 12px;
								}
							}
						}
						>a{
							border-left:none;
							padding:0;
						}
						
					}
					&.hover{
							cursor: pointer;
							
							ul {
								box-shadow: 0px 3px 4px rgba(0, 0, 0, 0.41);
							}
							ul:hover {
								display:block!important;
								
								
							}		
					}	

				}
				
			

			&.nav-sport{
				display:none;
				margin-left:10px;
					
					li{
						a{
							border-right: 1px solid grey;
							
						}
						&:last-child{
								a{
									border-right:none;
								}
							}
					}
				}

			> li{
				display: block;
				float: left;
				padding: 15px 0;
				margin-left:10px;
				
				&:first-child{
					margin-left:0;
				}
				

				a{
					color: @color-nav-main-link;
					font-size: 11px;
					padding: 5px 10px 5px 10px;
					font-weight: bold;
					
					&:hover{
						 color: @color-nav-main-link-hover;
					}

					&.active{
						color: @color-nav-main-link-active;
						background: @color-highlight;
						border-radius: 3px;
						border-right: none;
						padding: 5px 10px 5px 10px;
					}
				}
			}
		}
	}

	.active-nav-all{
		display: block;
	}


}

