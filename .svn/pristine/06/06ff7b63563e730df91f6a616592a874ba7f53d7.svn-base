body {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-size: 62.5%;
}
body a {
  color: #3e98c5;
  text-decoration: none;
}
body a:hover {
  text-decoration: underline;
}
body .header-small h1 {
  color: #3e98c5;
  font-weight: bold;
  padding: 0px 0 5px 0px;
  font-size: 16px;
}
body .header-small p {
  border-bottom: 2px solid #3e98c5;
  padding: 0 10px 8px 0px;
  margin-bottom: 10px;
  font-size: 22px;
  font-weight: bold;
  line-height: 0.9em;
}
body section header h2 {
  border-bottom: 2px solid #3e98c5;
  font-size: 18px;
  font-weight: bold;
  padding-bottom: 8px;
}
body section header h2 + h3 {
  padding-top: 8px;
}
body section .header-list {
  padding: 8px 8px;
  background: #efefef;
  margin-bottom: 4px;
}
body section .header-list h3 {
  color: #2b2b2b;
  font-weight: bold;
  font-size: 12px;
}
body section .header-list p {
  font-size: 0.9em;
  line-height: 1.6em;
}
body section p.more {
  clear: both;
  padding: 10px 3px;
}
body section p.more a {
  display: table-row;
}
body section p.more a:before {
  display: table-cell;
  height: 27px;
  width: 27px;
  text-align: center;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  background: #3e98c5;
  color: #ffffff;
  vertical-align: middle;
  font-weight: bold;
  font-size: 11px;
  content: "\f0c9";
  font-family: fontAwesome;
}
body section p.more a:before:hover {
  text-decoration: none;
}
body section p.more b {
  display: table-cell;
  vertical-align: middle;
  padding: 0 5px;
  font-weight: bold;
}
body .wrapper-page {
  font-size: 1.2em;
}
