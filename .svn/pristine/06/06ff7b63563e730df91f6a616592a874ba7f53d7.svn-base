.price .priceUp {
  animation: animatedShadowPulseGreen 1s linear infinite;
  -moz-animation: animatedShadowPulseGreen 1s linear infinite;
  -webkit-animation: animatedShadowPulseGreen 1s linear infinite;
  -ms-animation: animatedShadowPulseGreen 1s linear infinite;
  -o-animation: animatedShadowPulseGreen 1s linear infinite;
  border: 1px solid #6B932D!important;
}
.price .priceUp i,
.price .priceUp em {
  color: #6B932D!important;
}
.price .priceUp:after {
  content: '';
  position: relative;
  right: 7px;
  display: inline-block;
  float: right;
  color: #6BA80C!important;
  height: 22px;
  width: 25px;
  background-image: url('../images/arrow-up.png');
  background-position: center 20px;
  background-repeat: no-repeat;
  margin-top: -4px;
  margin-bottom: -4px;
  animation: animatedBackgroundUp 0.7s linear infinite;
  -moz-animation: animatedBackgroundUp 0.7s linear infinite;
  -webkit-animation: animatedBackgroundUp 0.7s linear infinite;
  -ms-animation: animatedBackgroundUp 0.7s linear infinite;
  -o-animation: animatedBackgroundUp 0.7s linear infinite;
}
.price .priceUp:hover {
  background: #E9F0DD!important;
}
.price .priceDown {
  animation: animatedShadowPulseRed 1s linear infinite;
  -moz-animation: animatedShadowPulseRed 1s linear infinite;
  -webkit-animation: animatedShadowPulseRed 1s linear infinite;
  -ms-animation: animatedShadowPulseRed 1s linear infinite;
  -o-animation: animatedShadowPulseRed 1s linear infinite;
}
.price .priceDown i,
.price .priceDown em {
  color: #EC1313!important;
}
.price .priceDown:after {
  content: '';
  position: relative;
  right: 7px;
  display: inline-block;
  float: right;
  color: #EC1313;
  height: 22px;
  width: 25px;
  background-image: url('../images/arrow-down.png');
  background-position: center;
  background-repeat: no-repeat;
  margin-top: -4px;
  margin-bottom: -4px;
  animation: animatedBackgroundDown 0.7s linear infinite;
  -moz-animation: animatedBackgroundDown 0.7s linear infinite;
  -webkit-animation: animatedBackgroundDown 0.7s linear infinite;
  -ms-animation: animatedBackgroundDown 0.7s linear infinite;
  -o-animation: animatedBackgroundDown 0.7s linear infinite;
}
.price .priceDown:hover {
  background: #FAF1F1!important;
}
