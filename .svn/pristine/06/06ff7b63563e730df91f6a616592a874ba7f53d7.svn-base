    width: 45%;
  }
  .market.top-5 li div.prices:nth-child(odd) {
    margin-left: 38px;
  }
  .market.top-5 li div.prices.single {
    clear: both;
    min-width: 93%;
    float: none;
    margin: 4px 0px 2px 38px;
  }
  .market.top-5 li div.dummy {
    clear: both;
    min-width: 90%;
    float: none;
    display: block;
    height: 0;
    padding: 0;
  }
  .market-container > li .market-title-selector p {
    margin-right: -5%;
  }
  .market-container > li .market-title-selector h2 {
    padding: 0 0 5px 0px;
  }
}
@media all and (max-width: 480px) {
  .market li {
    padding: 12px 0 4px 0;
  }
  .market li.inplay div.selection p {
    position: absolute;
    top: 5px;
  }
  .market li.inplay div.selection a {
    position: absolute;
    top: 22px;
    left: 5px;
  }
  .market li.inplay div.selection a span {
    font-size: 12px;
  }
  .market li.inplay div.time {
    min-width: 100px;
    text-align: left;
    margin-top: -12px;
    padding: 1px 0;
  }
  .market li.inplay div.time em {
    display: inline-block;
    padding-left: 10px;
    padding-right: 5px;
    font-size: 1em;
  }
  .market li.inplay div.time i {
    display: inline-block;
    line-height: 1.6em;
    font-size: 1em;
  }
  .market li div.selection {
    height: 32px;
    margin-left: 10%;
  }
  .market li div.selection a {
    position: absolute;
    top: 28px;
    left: 5px;
  }
  .market li div.selection a span {
    font-size: 12px;
  }
  .market li div.selection p {
    padding-bottom: 8px;
  }
  .market li div.selection span {
    font-size: 11px;
    line-height: 14px;
  }
  .market li div.selection span.team:last-child {
    display: inline;
  }
  .market li div.time {
    min-width: 50px;
    max-width: 70px;
    font-size: 11px;
  }
  .market li div.options {
    padding-right: 0;
    top: 4px;
  }
  .market li div.options a {
    padding: 2px;
  }
  .market.top-5 li div.prices {
    width: 44%;
  }
  .market.top-5 li div.prices.single {
    min-width: 88%;
  }
}
@media all and (max-width: 380px) {
  .market.top-5 li div.prices {
    width: 42%;
  }
  .market.top-5 .team span.score {
    float: right;
    padding-right: 10px;
  }
}
