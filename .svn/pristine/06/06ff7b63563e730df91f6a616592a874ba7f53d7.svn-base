}
.highlights.multiple ul li {
  display: inline-block;
  float: left;
  width: 32%;
  height: 90px;
  position: relative;
  margin-right: 1%;
}
.highlights.multiple ul li .hover {
  position: absolute;
  bottom: 0px;
  background: url("../images/opacity-80-white.png");
  width: 100%%;
  padding: 5px;
  height: 35px;
  overflow: hidden;
  -webkit-transition: all 0.5s ease-in-out;
  -moz-transition: all 0.5s ease-in-out;
  -o-transition: all 0.5s ease-in-out;
  transition: all 0.5s ease-in-out;
}
.highlights.multiple ul li .hover .when {
  display: inline-block;
  padding-left: 0;
  padding-right: 5%;
